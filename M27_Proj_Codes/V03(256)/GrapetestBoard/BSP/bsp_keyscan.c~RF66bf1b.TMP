//功能把缓冲区的浮点数更新到变量
void WritFP32TOvari(CPU_FP32 *Temp_dat,CPU_INT16U Add)
{
      CPU_INT16S   *LPtr_RegData=RegData;
      CPU_INT08U   i,*ptr,*ptr_mapreg;
      
      ptr=(CPU_INT08U*)(Temp_dat);
      ptr_mapreg=(CPU_INT08U*)(LPtr_RegData+Add);
        
      for(i=0;i<4;i++)
      {
            *(ptr+i)=*(ptr_mapreg+i);
      } 
}


